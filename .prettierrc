{
    "trailingComma": "none",
    "semi": true,
    "tabWidth": 2,
    "singleQuote": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "importOrder": [
      "^@core/(.*)$",
      "^@server/(.*)$",
      "^@ui/(.*)$",
      "^[./]"
